@inproceedings{alturki_design_2011,
	location = {Berlin, Heidelberg},
	title = {A Design Science Research Roadmap},
	isbn = {978-3-642-20633-7},
	doi = {10.1007/978-3-642-20633-7_8},
	series = {Lecture Notes in Computer Science},
	abstract = {This paper proposes and synthesizes from previous design science ({DS}) methodological literature a structured and detailed {DS} Roadmap for the conduct of {DS} research. The Roadmap is a general guide for researchers to carry out {DS} research by suggesting reasonably detailed activities. Though highly tentative, it is believed the Roadmap usefully inter-relates many otherwise seemingly disparate, overlapping or conflicting concepts. It is hoped the {DS} Roadmap will aid in the planning, execution and communication of {DS} research, while also attracting constructive criticism, improvements and extensions. A key distinction of the Roadmap from other {DS} research methods is its breadth of coverage of {DS} research aspects and activities; its detail and scope. We demonstrate and evaluate the Roadmap by presenting two case studies in terms of the {DS} Roadmap.},
	pages = {107--123},
	booktitle = {Service-Oriented Perspectives in Design Science Research},
	publisher = {Springer},
	author = {Alturki, Ahmad and Gable, Guy G. and Bandara, Wasana},
	editor = {Jain, Hemant and Sinha, Atish P. and Vitharana, Padmal},
	date = {2011},
	langid = {english},
	keywords = {Design Science, Archival Analysis, Design Research, Design Science research methodology, Design Science research Roadmap, Information System Design Theory},
	file = {Alturki et al. - 2011 - A Design Science Research Roadmap.pdf:/home/kevin/Zotero/storage/PXKL9LB8/Alturki et al. - 2011 - A Design Science Research Roadmap.pdf:application/pdf},
}

